[Disk summary] # caption
my.summary.disk.avgsz = delta(disk.dev.total_bytes) / delta(disk.dev.total)
my.summary.disk.avgsz[label] = sum
my.summary.disk.avgsz[color] = 1

my.metric.two = disk.dev.total_bytes
my.metric.two[label] = total
my.metric.two[color] = 2

my.metric.tree = disk.dev.total_bytes
my.metric.tree[label] = tree
my.metric.tree[color] = 2

